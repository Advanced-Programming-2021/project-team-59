<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="556.0" prefWidth="672.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Menu.SignUpAndLoginMenu">
    <children>
        <ImageView cache="true" fitHeight="556.0" fitWidth="679.0" layoutX="-1.0" mouseTransparent="true"
                   pickOnBounds="true">
            <image>
                <Image url="@../images/background.jpg"/>
            </image>
        </ImageView>
        <Button layoutX="223.0" layoutY="261.0" mnemonicParsing="false" onAction="#openRegisterMenu"
                onMouseClicked="#openRegisterMenu" prefHeight="35.0" prefWidth="116.0"
                style="-fx-cursor: HAND; -fx-border-style: solid; -fx-border-width: 2; -fx-background-color: white; -fx-border-color: orange;"
                text="sign up" textAlignment="CENTER">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
        <Button layoutX="223.0" layoutY="309.0" mnemonicParsing="false" onAction="#openLoginMenu"
                onMouseClicked="#openLoginMenu" prefHeight="31.0" prefWidth="116.0"
                style="-fx-background-color: white; -fx-border-color: orange; -fx-border-style: solid; -fx-border-width: 2; -fx-cursor: HAND;"
                text="login" textAlignment="CENTER">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
